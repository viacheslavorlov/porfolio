{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/src/content/posts",
      "contentTypes": [
        "posts"
      ]
    },
    {
      "title": "technologies",
      "path": "[[workspace]]/src/content/technologies",
      "contentTypes": [
        "technologies"
      ]
    }
  ],
  "frontMatter.content.supportedFileTypes": [ "md", "markdown", "mdx", "mdoc" ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "posts",
      "previewPath": "'posts'",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        }
      ]
    },
    {
      "name": "technologies",
      "previewPath": "'technologies'",
      "pageBundle": false,
      "clearEmpty": true,
      "fields": [
        {
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "name": "icon",
          "type": "string",
          "single": true,
          "required": true
        }
      ]
    }
  ],
  "frontMatter.git.enabled": true
}