---
import Menu from 'src/widgets/menu/Menu.astro';
import MobileMenu from './MobileMenu';
import Section from './Section.astro';

const { phone } = Astro.props;
---

<Section paddings={[2, 4, 4]} className={'width-screen bg-primary-bg'}>
<div class='mx-auto w-full max-w-[1280px] flex justify-between items-center gap-10 p-8'>
		<div class='md:flex justify-center items-center hidden mr-auto font-semibold gap-2 w-full'>
			<Menu />
		</div>

		<MobileMenu client:load />
	</div>
</Section>
